#!/usr/bin/env bash
# This script kills the process named "7-highlander" if it is running

# Check if the process is running
if pgrep -x "7-highlander" >/dev/null; then
    # If running, kill the process
    pkill -x "7-highlander"
    echo "Process '7-highlander' has been killed."
else
    # If not running, display message
    echo "Process '7-highlander' is not running."
fi

